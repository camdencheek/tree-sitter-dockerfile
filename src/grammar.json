{
  "name": "dockerfile",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_instruction"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "_instruction": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "from_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "run_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "cmd_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "label_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "expose_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "env_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "add_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "copy_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "entrypoint_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "volume_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "user_instruction"
