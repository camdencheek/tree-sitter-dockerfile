        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[aA][sS]"
                  },
                  "named": false,
                  "value": "AS"
                },
                {
                  "type": "FIELD",
                  "name": "as",
                  "content": {
                    "type": "SYMBOL",
                    "name": "image_alias"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "run_instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[rR][uU][nN]"
          },
          "named": false,
          "value": "RUN"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
