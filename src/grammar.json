        {
          "type": "SYMBOL",
          "name": "onbuild_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "stopsignal_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "healthcheck_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "shell_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "maintainer_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "cross_build_instruction"
        }
      ]
    },
    "from_instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[fF][rR][oO][mM]"
          },
          "named": false,
          "value": "FROM"
